@model Movie
<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" type="text/css" href="">
    <title>Movie Recommendation</title>
</head>
<body>
    <main>
        <p>@Model.Title</p>
        <p>@Model.Runtime Minutes</p>
        <p>@Model.Overview</p>
        @for (int i = 0; i < @Model.Genres.Count(); i++)
        {
          <p>@Model.Genres[i]</p>
        }
        <p>https://image.tmdb.org/t/p/original/@Model.Poster_Path</p>
        
        @if (@Model.Images == null)
        {
            
            <p>Nothing Grabbed.</p>
        }
        <img width="300" height="300" src="https://image.tmdb.org/t/p/original/@Model.Poster_Path" />
    </main>
</body>
</html>